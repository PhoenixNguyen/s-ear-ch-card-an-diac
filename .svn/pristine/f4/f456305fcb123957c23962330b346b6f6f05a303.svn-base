#account-header {
	padding-top: 45px;
	font-size:13px;
	float: right;
}
#account-header .nav {
	margin-top:0;
	padding: 0px;
	list-style:none
}

#account-header .nav li:hover .account_dropdown {
	display:block
}
#account-header .nav li {
	float:left;
	list-style:none;
	position:relative;
	z-index:10;
}
#account-header .nav li .header-nav-link {
	background: url("../images/icon_drop.png") no-repeat scroll right center transparent;
    display: block;
    margin-right: 5px;
    padding: 9px 15px 9px 5px;
    position: relative;
	font-weight: bold;
}
#account-header .nav li .header-nav-link img {
	margin-top:-5px
}
div#account-header li.active .sub-nav {
	display:block
}
div#account-header .sub-nav {
	display:none;
	position:absolute;
	right:0;
	top:100%;
	margin-top:12px;
	min-width:165px;
	max-width:250px;
	z-index:1;
}
div#account-header .sub-nav ul {
	margin:0;
	padding:0
}
div#account-header .sub-nav ul li {
	margin:0;
	padding:0;
	display:block;
	float:none
}
div#account-header .sub-nav ul li .name {
	font-size:13px;
	font-weight:600;
	color:#000;
	margin-bottom:6px
}
div#account-header .sub-nav ul li .email {
	color:#222
}
div#account-header .sub-nav ul li .quota-text {
	color:#888
}
div#account-header .sub-nav ul li .email, div#account-header .sub-nav ul li .quota-text {
	font-size:13px;
	margin-bottom:6px
}
div#account-header .sub-nav ul li a {
	display:block;
	padding:7px 20px;
	margin:0 -20px;
	color:#333;
	zoom:1;
	 overflow: hidden;
	min-width:165px
}
div#account-header .sub-nav ul li a img {
	margin-right:6px;
	margin-bottom:-3px
}
div#account-header .sub-nav ul li a:hover {
	background-color:#effff1;
	text-decoration:none
}
div#account-header .sub-nav ul li .quota_graph_container {
	margin:16px 0;
	border-top:1px solid #ddd;
	background:#eee;
	height:5px;
	width:165px
}
